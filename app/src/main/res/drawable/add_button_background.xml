<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#146031" />
            <gradient android:angle="-90" android:endColor="#90edb3" android:startColor="#345641" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#146031" />
            <solid android:color="#32f27c" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#146031" />
            <gradient android:angle="-90" android:endColor="#345641" android:startColor="#90edb3" />
        </shape>
    </item>
</selector>